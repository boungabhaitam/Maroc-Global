{
  "expo": {
    "name": "Maroc Global",
    "slug": "maroc-global",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1.png",
        "backgroundColor": "#333333"
      },
      "package": "com.pouxulis.marocglobal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
